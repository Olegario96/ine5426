void main() {
	for (int i = 0; i <= 10; i = i+1) {
    	switch (i) {
        	case (i < 5) {
            	int j = i*2;
            	print ("First case: ");
            	print (j / 2);
            	print ("\n");
        	}
        	case (i < 10) {
            	int j = i+15;
            	print ("Second case: ");
            	print (j);
            	print ("\n");
        	}
        	default {
            		print ("Default");
        	}
    	}
	}
}
